<h2>Listing enrollments</h2>

<table class="table">
  <thead>
    <tr>
      <th>Student</th>
      <th>Class</th>

      <th></th>
    </tr>
  </thead>
  <tbody>
<%= for enrollment <- @enrollments do %>
    <tr>
      <td><%= enrollment.student_id %></td>
      <td><%= enrollment.class_id %></td>

      <td class="text-right">
        <%= link "Show", to: enrollment_path(@conn, :show, enrollment), class: "btn btn-default btn-xs" %>
        <%= link "Edit", to: enrollment_path(@conn, :edit, enrollment), class: "btn btn-default btn-xs" %>
        <%= link "Delete", to: enrollment_path(@conn, :delete, enrollment), method: :delete, data: [confirm: "Are you sure?"], class: "btn btn-danger btn-xs" %>
      </td>
    </tr>
<% end %>
  </tbody>
</table>

<%= link "New enrollment", to: enrollment_path(@conn, :new) %>
